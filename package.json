{
  "name": "nls",
  "version": "3.1.1",
  "description": "Missing inspector for npm packages.",
  "repository": "https://github.com/amio/nls",
  "author": "Amio <amio.cn@gmail.com>",
  "license": "MIT",
  "bin": {
    "nls": "dist/index.js"
  },
  "scripts": {
    "lint": "standard lib/*",
    "pretest": "npm run lint",
    "test": "tap test/*.spec.js --reporter spec --test-env=FORCE_COLOR=0 --no-check-coverage",
    "snap": "TAP_SNAPSHOT=1 npm run test",
    "build": "ncc build lib/nls -m",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^16.18.11",
    "@zeit/ncc": "^0.22.1",
    "columnify": "^1.5.4",
    "execa": "^5.0.0",
    "kleur": "^4.1.3",
    "marked": "^4.2.5",
    "marked-terminal": "^5.1.1",
    "mri": "^1.1.5",
    "npm-why": "^5.0.0",
    "standard": "^17.0.0",
    "tap": "^16.3.3",
    "yarn-why": "^0.3.2"
  },
  "engines": {
    "node": ">=16"
  },
  "keywords": [
    "list",
    "npm",
    "scripts"
  ]
}
